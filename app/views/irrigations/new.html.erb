<h1>NEW Irrigation for <%=@pmu.location%></h1>

<%=form_for ([@pmu,@irrigation]) do |f| %>
<fieldset>
	<ol>
		<li>
			<%=f.label :date%>
			<%=f.date_field :date %>
		</li>
		<li>
			<%=f.label :duration%>
			<%=f.text_field :duration%>
		</li>
		<li>
			<%=f.label :quantity%>
			<%=f.text_field :quantity %>
		</li>
		<li>
			<%=f.label :operator%>
			<%=f.text_field :operator%>
		</li>
	</ol>
	<p>
		<%=f.submit%>
		<%=link_to "Cancel",pmu_irrigations_path %>
	</p>
</fieldset>

<% end%>
