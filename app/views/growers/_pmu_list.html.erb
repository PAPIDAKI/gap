<%  pmus.each  do |pmu|   %>
    <table class="table">
      <tr>
        <td><%= link_to pmu.location,grower_pmu_path(@grower,pmu) %></td>

        <td><%= pmu.produce %></td>
        <td><%= pmu.variety %></td>
        <td><%= pmu.expected_production %></td>
        <td><%= link_to "Edit",edit_grower_pmu_path(@grower,pmu)%></td>
        <td><%=link_to "Delete",grower_pmu_path(@grower,pmu),method: :delete %></td>
        
      </tr>
  </table>

    <% end %>